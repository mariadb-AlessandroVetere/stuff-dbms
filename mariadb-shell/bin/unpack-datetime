#!/usr/bin/perl
use Date::Format;

foreach my $hex (@ARGV)
{
    my $t = hex($hex);
    my $frac = $t % 1000000; $t /= 1000000;
    my $sec = $t % 60; $t /= 60;
    my $min = $t % 60; $t /= 60;
    my $hr = $t % 24; $t /= 24;
    my $day = $t % 32; $t /= 32;
    my $mon = $t % 13; $t /= 13;
    my $year = int($t);
    my @lt = ($sec, $min, $hr, $day, $mon - 1, $year - 1900);
    print strftime("%Y-%m-%d %H:%M:%S.${frac}\n", @lt);
}
